  "devDependencies": {
    "@google-cloud/compute": "^0.10.0",
    "@google-cloud/container": "^0.2.0",
    "@google-cloud/storage": "^1.7.0",
    "@rockholla/git-npm-release": "^0.6.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "jest": "^23.1.0",
    "webpack": "^4.12.0",
    "winston": "^3.0.0"
