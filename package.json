{
  "name": "@rockholla/gcp",
  "version": "0.0.0",
  "description": "An extension of Google Cloud SDK components, functionality and helpers not available through the SDK itself",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockholla/nodejs-gcp.git"
  },
  "author": "Rockholla, LLC",
  "contributors": [
