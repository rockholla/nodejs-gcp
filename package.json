  "bugs": {
    "url": "https://github.com/rockholla/nodejs-gcp/issues"
  },
  "homepage": "https://github.com/rockholla/nodejs-gcp#readme",
  "main": "lib/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "scripts": {
